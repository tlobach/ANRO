
<robot name="test_robot">
  <link name="base_link">
        <visual>
                <geometry>
                        <cylinder length="0.6" radius="0.2"/>
                </geometry>
		<material name="white">
        	<color rgba="1 1 1 1"/>
      		</material>
        </visual>
  </link>

  <link name="arm1">
	<visual>
		<geometry>
			<box size = "1 .1 .1"/>
		</geometry>
		<material name="white">
        	<color rgba="1 1 1 1"/>
      		</material>
		<origin xyz = "0.5 0 0"/>
	</visual>
  </link>
    
  <link name="link1">
	<visual>
		<geometry>
			<cylinder length="0.6" radius="0.2"/>
		</geometry>
		<material name="blue">
        	<color rgba="0 0 1 1"/>
      		</material>
	</visual>
  </link>

  <joint name="link1_to_arm1" type="fixed">
    <parent link = "link1"/>
    <child link = "arm2"/>
  </joint>

  <link name="arm2">
        <visual>
                <geometry>
                        <box size = "3 .1 .1"/>
                </geometry>
		<material name="blue">
        	<color rgba="0 0 1 1"/>
      		</material>
                <origin xyz = "-1.5 0 0"/>
        </visual>
  </link>


  <link name="link2">
        <visual>
                <geometry>
                        <cylinder length="0.6" radius="0.2"/>
                </geometry>
		<origin xyz = "-3 0 0"/>
        </visual>
  </link>

  <joint name="link2_to_arm2" type="fixed">
    <parent link = "arm2"/>
    <child link = "link2"/>
  </joint>

  <link name="arm3">
        <visual>
                <geometry>
                        <box size = ".02 .02 3"/>
                </geometry>
		<material name="lime">
        	<color rgba="0 255 0 1"/>
      		</material>
                <origin xyz = "0 0 -1.5"/>
        </visual>
  </link>


  <link name="link3">
        <visual>
                <geometry>
                        <sphere radius="0.2"/>
                </geometry>
		<material name="lime">
        	<color rgba="0 255 0 1"/>
      		</material>
        </visual>
  </link>

  <joint name="link3_to_arm3" type="fixed">
    <parent link = "link3"/>
    <child link = "arm3"/>
  </joint>


  <joint name="joint1" type="continuous">
    <parent link="base_link"/>
    <child link="arm1"/>
    <origin xyz="0 0 0" rpy="0 -0 0.6268319" />
    <axis xyz="0 0 1" />
  </joint>

  <joint name="joint2" type="revolute">
    <parent link="arm1"/>
    <child link="link1"/>
    <limit effort="1000.0" lower="-2.9" upper="2.9" velocity="0.5"/>
    <origin xyz="1.0 0 0" rpy="3.14159 -0 3.14159" />
    <axis xyz="0 0 1" />
  </joint>

  <joint name="joint3" type="prismatic">
    <parent link="link2"/>
    <child link="link3"/>
    <limit effort="1000.0" lower="0.3" upper="3" velocity="0.5"/>
    <origin xyz="-3 0 0" rpy="0 0 0" />
    <axis xyz="0 0 1" />
  </joint>
</robot>
